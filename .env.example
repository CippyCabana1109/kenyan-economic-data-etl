# Google Cloud Configuration
GOOGLE_PROJECT_ID=your-gcp-project-id
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json

# BigQuery Settings
BIGQUERY_DATASET=kenyan_economic_data
BIGQUERY_TABLE=kenyan_gdp

# KNBS Data Source
KNBS_API_URL=https://www.knbs.or.ke/wp-content/uploads/2023/03/GCP-2022-Report-FINAL.pdf
KNBS_API_KEY=your-knbs-api-key-if-needed

# Airflow Configuration
AIRFLOW_HOME=/opt/airflow
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__CORE__SQL_ALCHEMY_CONN=sqlite:///airflow.db

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/etl_pipeline.log

# Performance Settings
CHUNK_SIZE=10000
MAX_WORKERS=4

# Data Quality Settings
MIN_ROWS_REQUIRED=40
MAX_NULL_THRESHOLD=0.1
